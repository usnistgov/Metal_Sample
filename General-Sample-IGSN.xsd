      <xs:element name="SampleNew" type="MaterialType"/>
      <xs:complexType name="MaterialType">
            <xs:sequence>
                  <xs:element minOccurs="1" name="SampleID" type="thing" maxOccurs="unbounded"/>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="ParentSamples" type="thing"/>
