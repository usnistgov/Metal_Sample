                        <xs:annotation>
                              <xs:appinfo/>
                        </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="MaterialCrystallinity" type="FormOfMaterial"/>
                  <xs:element maxOccurs="1" minOccurs="0" name="Phase" type="PhaseType"/>
