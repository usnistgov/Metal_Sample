                        <xs:complexType>
                              <xs:sequence>
                                    <xs:element name="OrganizationName" type="xs:string"/>
                                    <xs:element name="IddNumber" type="xs:string"/>
                              </xs:sequence>
                        </xs:complexType>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="SampleLocation"
