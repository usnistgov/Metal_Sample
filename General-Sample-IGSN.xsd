                              <xs:sequence>
                                    <xs:element name="MeasuredComposition" type="CompositionType">
                                          <xs:annotation>
                                                <xs:appinfo/>
                                          </xs:annotation>
                                    </xs:element>
                                    <xs:element name="CompositionMeasurementMethod" type="xs:string"/>
                                    <xs:element maxOccurs="1" minOccurs="0" name="MeasurementDate"
                                          type="xs:date"/>
                              </xs:sequence>
                        </xs:complexType>
                  </xs:element>
                  <xs:element name="Processing" type="AlloyProcessingType" minOccurs="1"/>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="SampleNotes">
                        <xs:complexType>
                              <xs:sequence>
                                    <xs:element name="SampleNote" type="note-type"/>
                                    <xs:element name="Downloadfiles" type="remote-file-type"/>
                              </xs:sequence>
                        </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="PrintingNotes" type="note-type">
                        <xs:annotation>
                              <xs:appinfo><label>Label Notes: Enter information to print on sample label under Title</label>
                              </xs:appinfo>
                        </xs:annotation>
                  </xs:element>
            </xs:sequence>
            <xs:attribute name="persistid" type="xs:string">
                  <xs:annotation>
                        <xs:appinfo><placeholder>automatically filled when record is stored</placeholder>
                        </xs:appinfo>
                  </xs:annotation>
